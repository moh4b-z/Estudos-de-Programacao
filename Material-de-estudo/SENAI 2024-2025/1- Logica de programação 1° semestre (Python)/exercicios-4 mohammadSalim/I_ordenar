A = float(input('Digite o número de A: '))
B = float(input('Digite o número de B: '))
C = float(input('Digite o número de C: '))
D = float(input('Digite o número de D: '))
E = float(input('Digite o número de E: '))

Numeros = [A, B, C, D, E]
maior_numero = Numeros[0]
menor_numero = Numeros[0]

for numero in Numeros:
    if numero > maior_numero:
        maior_numero = numero
        
for numero in Numeros:
    if numero < menor_numero:
        menor_numero = numero
        
print(f'O maior número é: {maior_numero} e o menor {menor_numero}')
